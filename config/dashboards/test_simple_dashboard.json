{
  "dashboard": {
    "id": null,
    "title": "NCS Live Test Dashboard",
    "tags": ["ncs", "test"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-5m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Control System Stability",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "uid": "influxdb-ncs-uid"
            },
            "query": "SELECT mean(stability_margin) FROM control_kpis WHERE $timeFilter GROUP BY time($__interval) fill(null)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Agent MTTR",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "uid": "influxdb-ncs-uid"
            },
            "query": "SELECT last(mttr) FROM agent_kpis",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      }
    ]
  },
  "overwrite": true
}
